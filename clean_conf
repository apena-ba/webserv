server {
    default_error_page : page/error.html;
    client_body_max_size : 22;
    max_clients : 10;
    host : 0.0.0.0;
    ports : 8083;
    root: /Users/emilefournout/webservcleaning/html;
    index: index.html;

    route{
        methods : get,DELETE;
        location : /;
        index : index.html;
    }
    route{
        methods : get,DELETE;
        location : /default;
        index : file.html;
    }
    route {
        methods : get,DELETE;
        location : /somepath;
        index : index.html;
    }
    route {
        methods : get,DELETE;
        location : /download;
        index : downloaded.html;
    }
}

